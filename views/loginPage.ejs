<%- include('./layouts/header'); -%>
<link rel="stylesheet" href="/css/loginPage.css" />
<body>
  <main>
    <div class="content">
      <div class="content-input">
        <p style="font-size: 1.1rem; color: rgb(228, 228, 228);">MASUK KE LMS</p>
        <img src="/img/logo.png" alt="" />
        <form id="formLogin">
          <div class="input">
            <i class="uil uil-user"></i>
            <input type="text" required name="nuptk" placeholder="NUPTK" />
          </div>
          <div class="input">
            <i class="uil uil-lock"></i>
            <input
              type="password"
              required
              name="password"
              placeholder="Password"
            />
          </div>
      </div>
        <button class="loginButton">MASUK</button>
      </form>
    </div>
  </main>
</body>
<%- include('./layouts/footer'); -%>
<script src="/js/loginPage.js"></script>
